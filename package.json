{
  "name": "@stitch-ai/agent-server",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "local": "dotenv -e .env -- nest start --watch",
    "build": "dotenv -e .env -- nest build",
    "start": "dotenv -e .env -- node dist/main.js",
    "prisma:push": "dotenv -e .env -- prisma db push",
    "prisma:pull": "dotenv -e .env -- prisma db pull",
    "prisma:generate": "dotenv -e .env -- prisma generate",
    "prisma:reset": "dotenv -e .env -- prisma migrate reset --skip-seed",
    "prisma:seed": "dotenv -e .env -- ts-node src/providers/prisma/seeds",
    "check:prettier": "prettier --write \"src/**/*.ts\"",
    "check:lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "check:type": "tsc --p ./tsconfig.json --noEmit"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/swagger": "^7.1.13",
    "@prisma/client": "^5.4.2",
    "axios": "^1.5.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "discord-webhook-node": "^1.1.8",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "nest-winston": "^1.9.4",
    "number-precision": "^1.6.0",
    "p-limit": "^3.1.0",
    "prisma": "^5.4.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "supertest": "^6.3.3",
    "viem": "^1.19.9",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@types/cookie-parser": "^1.4.5",
    "@types/cron": "^2.4.0",
    "@types/crypto-js": "^4.1.3",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "schema": "src/providers/prisma/schema/schema.prisma"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/$1"
    }
  },
  "packageManager": "yarn@3.6.4"
}
